    "nodemailer": "^6.9.5",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
